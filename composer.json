{
    "name": "phpcompose/compose",
    "type": "library",
    "description": "Pages-first PHP framework: filesystem-driven routing, PSR-15 pipeline, pragmatic DI container, and simple view engine.",
    "support": {
        "docs": "https://phpcompose.github.io/compose/",
        "source": "https://github.com/phpcompose/compose"
    },
        "homepage": "https://phpcompose.github.io/compose/",
    "license": "MIT",
    "keywords": [
        "php",
        "framework",
        "psr-15",
        "middleware",
        "pages",
        "mvc",
        "container",
        "dependency-injection",
        "view",
        "routing"
    ],
    "authors": [
        {
            "name": "Alamin Ahmed",
            "email": "alamin@phpcompose.org",
            "role": "lead"
        }
    ],
    "autoload": {
        "psr-4": {
            "Compose\\": "src/"
        }
    },
    "require": {
        "php": "~8.3",
        "laminas/laminas-config-aggregator": "^1.1",
        "laminas/laminas-diactoros": "^3.6",
        "laminas/laminas-httphandlerrunner": "^2.10",
        "laminas/laminas-stratigility": "^4.2",
        "league/plates": "^3.6",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "scripts": {
        "test-all": "phpunit --colors=always --testdox --bootstrap vendor/autoload.php tests",
        "test": "phpunit --colors=always --testdox --bootstrap vendor/autoload.php tests",
        "serve": "php -S 0.0.0.0:8080 -t public/"
    },
    "config": {
        "platform": {
            "php": "8.3.0"
        },
        "sort-packages": true
    },
    
}
